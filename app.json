{
  "expo": {
    "name": "Alexey's Recipe App",
    "slug": "RecipeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/chef-icon.png",
    "splash": {
      "image": "./assets/chef-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gorobots.alexeysrecipeapp",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/transparent-chef.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.gorobots.alexeysrecipeapp",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/chef-icon.png"
    }
  }
}
